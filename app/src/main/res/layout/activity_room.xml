<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="#FFFFFF"    android:scrollbarSize="4dp"    tools:context="com.example.smartoffice3.activitys.Room">    <Button        android:id="@+id/button_hum3"        style="?android:attr/borderlessButtonStyle"        android:layout_width="wrap_content"        android:layout_height="94dp"        android:layout_below="@+id/button_hum2"        android:layout_alignParentStart="true"        android:layout_alignParentLeft="true"        android:layout_alignParentEnd="true"        android:layout_alignParentRight="true"        android:layout_marginStart="78dp"        android:layout_marginLeft="78dp"        android:layout_marginTop="1dp"        android:layout_marginEnd="-1dp"        android:layout_marginRight="-1dp"        android:background="#FFFFFF"        android:drawableBottom="@drawable/hairline"        android:text="Button" />    <Button        android:id="@+id/button_hum2"        style="?android:attr/borderlessButtonStyle"        android:layout_width="wrap_content"        android:layout_height="94dp"        android:layout_below="@+id/button_hum"        android:layout_alignParentStart="true"        android:layout_alignParentLeft="true"        android:layout_alignParentEnd="true"        android:layout_alignParentRight="true"        android:layout_marginStart="78dp"        android:layout_marginLeft="78dp"        android:layout_marginTop="1dp"        android:layout_marginEnd="-1dp"        android:layout_marginRight="-1dp"        android:background="#FFFFFF"        android:drawableBottom="@drawable/hairline"        android:text="Button" />    <Button        android:id="@+id/button_hum"        style="?android:attr/borderlessButtonStyle"        android:layout_width="wrap_content"        android:layout_height="94dp"        android:layout_below="@+id/too_hot_text_v"        android:layout_alignParentStart="true"        android:layout_alignParentLeft="true"        android:layout_alignParentEnd="true"        android:layout_alignParentRight="true"        android:layout_marginStart="78dp"        android:layout_marginLeft="78dp"        android:layout_marginTop="1dp"        android:layout_marginEnd="-1dp"        android:layout_marginRight="-1dp"        android:background="#FFFFFF"        android:drawableBottom="@drawable/hairline"        android:text="Button" />    <TextView        android:id="@+id/name_room"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_below="@+id/face"        android:layout_alignParentStart="true"        android:layout_alignParentLeft="true"        android:layout_alignParentEnd="true"        android:layout_alignParentRight="true"        android:layout_marginStart="0dp"        android:layout_marginLeft="0dp"        android:layout_marginTop="20dp"        android:layout_marginEnd="0dp"        android:layout_marginRight="0dp"        android:gravity="center"        android:text="TextView"        android:textColor="@android:color/black"        android:textSize="24sp" />    <ImageButton        android:id="@+id/face"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentStart="true"        android:layout_alignParentLeft="true"        android:layout_alignParentTop="true"        android:layout_alignParentEnd="true"        android:layout_alignParentRight="true"        android:layout_marginStart="121dp"        android:layout_marginLeft="121.5dp"        android:layout_marginTop="50dp"        android:layout_marginEnd="122dp"        android:layout_marginRight="121.5dp"        android:background="#00FFFFFF"        app:srcCompat="@drawable/face_room" />    <TextView        android:id="@+id/look_room"        android:layout_width="match_parent"        android:layout_height="30dp"        android:layout_below="@+id/name_room"        android:layout_alignParentStart="true"        android:layout_alignParentLeft="true"        android:layout_alignParentEnd="true"        android:layout_alignParentRight="true"        android:layout_marginStart="0dp"        android:layout_marginLeft="0dp"        android:layout_marginTop="14dp"        android:layout_marginEnd="0dp"        android:layout_marginRight="0dp"        android:gravity="center"        android:text="TextView"        android:textColor="@android:color/black" />    <Button        android:id="@+id/too_hot_text_v"        style="?android:attr/borderlessButtonStyle"        android:layout_width="wrap_content"        android:layout_height="94dp"        android:layout_below="@+id/look_room"        android:layout_alignParentStart="true"        android:layout_alignParentLeft="true"        android:layout_alignParentEnd="true"        android:layout_alignParentRight="true"        android:layout_marginStart="79dp"        android:layout_marginLeft="79dp"        android:layout_marginTop="10dp"        android:layout_marginEnd="-2dp"        android:layout_marginRight="-2dp"        android:background="#FFFFFF"        android:drawableBottom="@drawable/hairline"        android:text="Button"        android:textSize="11sp" />    <ImageButton        android:id="@+id/temp_image"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignTop="@+id/too_hot_text_v"        android:layout_alignParentStart="true"        android:layout_alignParentLeft="true"        android:layout_marginStart="19dp"        android:layout_marginLeft="20dp"        android:layout_marginTop="29dp"        android:background="#00FFFFFF"        app:srcCompat="@drawable/temperature" />    <ImageButton        android:id="@+id/union_image"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignTop="@+id/button_hum"        android:layout_alignParentStart="true"        android:layout_alignParentLeft="true"        android:layout_marginStart="20dp"        android:layout_marginLeft="20dp"        android:layout_marginTop="33dp"        android:background="#00FFFFFF"        app:srcCompat="@drawable/union" />    <ImageButton        android:id="@+id/vector_image"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_below="@+id/button_hum"        android:layout_alignParentStart="true"        android:layout_alignParentLeft="true"        android:layout_marginStart="22dp"        android:layout_marginLeft="22dp"        android:layout_marginTop="29dp"        android:background="#00FFFFFF"        app:srcCompat="@drawable/vector" />    <ImageButton        android:id="@+id/bright_image"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignTop="@+id/button_hum3"        android:layout_alignParentStart="true"        android:layout_alignParentLeft="true"        android:layout_marginStart="20dp"        android:layout_marginLeft="20dp"        android:layout_marginTop="26dp"        android:background="#00FFFFFF"        app:srcCompat="@drawable/brightness" />    <ImageButton        android:id="@+id/back_button"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentStart="true"        android:layout_alignParentLeft="true"        android:layout_alignParentTop="true"        android:layout_marginStart="10dp"        android:layout_marginLeft="10dp"        android:layout_marginTop="11dp"        android:background="#00330000"        app:srcCompat="@drawable/back" />    <ImageButton        android:id="@+id/text_back"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignTop="@+id/back_button"        android:layout_marginStart="0dp"        android:layout_marginLeft="0dp"        android:layout_marginTop="5dp"        android:layout_toEndOf="@+id/back_button"        android:layout_toRightOf="@+id/back_button"        android:background="#00FFFFFF"        app:srcCompat="@drawable/back_text" />    <ImageButton        android:id="@+id/edit_button"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentTop="true"        android:layout_alignParentEnd="true"        android:layout_alignParentRight="true"        android:layout_marginTop="15dp"        android:layout_marginEnd="19dp"        android:layout_marginRight="10dp"        android:background="#00FFFFFF"        app:srcCompat="@drawable/edit" />    <TextView        android:id="@+id/temperature_textView"        android:layout_width="134dp"        android:layout_height="wrap_content"        android:layout_alignStart="@+id/too_hot_text_v"        android:layout_alignLeft="@+id/too_hot_text_v"        android:layout_alignTop="@+id/too_hot_text_v"        android:layout_marginStart="10dp"        android:layout_marginLeft="10dp"        android:layout_marginTop="10dp"        android:text="Temperature"        android:textColor="#000000"        android:textSize="11sp" />    <TextView        android:id="@+id/humidity_textView"        android:layout_width="146dp"        android:layout_height="wrap_content"        android:layout_alignStart="@+id/button_hum"        android:layout_alignLeft="@+id/button_hum"        android:layout_alignTop="@+id/button_hum"        android:layout_marginStart="12dp"        android:layout_marginLeft="1dp"        android:layout_marginTop="10dp"        android:text="Humidity"        android:textColor="#000000"        android:textSize="11sp" />    <TextView        android:id="@+id/co2_textView"        android:layout_width="100dp"        android:layout_height="wrap_content"        android:layout_alignStart="@+id/button_hum2"        android:layout_alignLeft="@+id/button_hum2"        android:layout_alignTop="@+id/button_hum2"        android:layout_marginStart="12dp"        android:layout_marginLeft="12dp"        android:layout_marginTop="10dp"        android:text="CO2 Level"        android:textColor="#000000"        android:textSize="11sp" />    <TextView        android:id="@+id/brichtness_textView"        android:layout_width="179dp"        android:layout_height="wrap_content"        android:layout_alignStart="@+id/button_hum3"        android:layout_alignLeft="@+id/button_hum3"        android:layout_alignTop="@+id/button_hum3"        android:layout_marginStart="11dp"        android:layout_marginLeft="11dp"        android:layout_marginTop="10dp"        android:text="Brichtness"        android:textColor="#000000"        android:textSize="11sp" />    <TextView        android:id="@+id/temperature_k_text"        android:layout_width="65dp"        android:layout_height="30dp"        android:layout_alignStart="@+id/too_hot_text_v"        android:layout_alignLeft="@+id/too_hot_text_v"        android:layout_alignTop="@+id/too_hot_text_v"        android:layout_marginStart="10dp"        android:layout_marginLeft="10dp"        android:layout_marginTop="30dp"        android:text="30.60"        android:textColor="#000000"        android:textSize="24sp" />    <TextView        android:id="@+id/oc_textView"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignBottom="@+id/temperature_k_text"        android:layout_marginStart="0dp"        android:layout_marginLeft="0dp"        android:layout_marginBottom="1dp"        android:layout_toEndOf="@+id/temperature_k_text"        android:layout_toRightOf="@+id/temperature_k_text"        android:text="°C"        android:textColor="#CD000000" />    <TextView        android:id="@+id/humidity_k_text"        android:layout_width="65dp"        android:layout_height="30dp"        android:layout_alignStart="@+id/button_hum"        android:layout_alignLeft="@+id/button_hum"        android:layout_alignTop="@+id/button_hum"        android:layout_marginStart="12dp"        android:layout_marginLeft="12dp"        android:layout_marginTop="30dp"        android:text="40.12"        android:textColor="#000000"        android:textSize="24sp" />    <TextView        android:id="@+id/proc_textView"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignBottom="@+id/humidity_k_text"        android:layout_marginStart="0dp"        android:layout_marginLeft="0dp"        android:layout_marginBottom="2dp"        android:layout_toEndOf="@+id/humidity_k_text"        android:layout_toRightOf="@+id/humidity_k_text"        android:text="%"        android:textColor="#CE000000" />    <TextView        android:id="@+id/co2_k_text"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignStart="@+id/button_hum2"        android:layout_alignLeft="@+id/button_hum2"        android:layout_alignTop="@+id/button_hum2"        android:layout_marginStart="12dp"        android:layout_marginLeft="12dp"        android:layout_marginTop="30dp"        android:text="1220.16"        android:textColor="#000000"        android:textSize="24sp" />    <TextView        android:id="@+id/ppm_textView"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignBottom="@+id/co2_k_text"        android:layout_marginStart="3dp"        android:layout_marginLeft="3dp"        android:layout_marginBottom="1dp"        android:layout_toEndOf="@+id/co2_k_text"        android:layout_toRightOf="@+id/co2_k_text"        android:text="ppm"        android:textColor="#CE000000" />    <TextView        android:id="@+id/bricht_k_text"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_below="@+id/button_hum2"        android:layout_alignStart="@+id/button_hum3"        android:layout_alignLeft="@+id/button_hum3"        android:layout_marginStart="11dp"        android:layout_marginLeft="11dp"        android:layout_marginTop="28dp"        android:text="60"        android:textColor="#000000"        android:textSize="24sp" />    <TextView        android:id="@+id/lx_textView"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignBottom="@+id/bricht_k_text"        android:layout_marginStart="4dp"        android:layout_marginLeft="4dp"        android:layout_marginBottom="2dp"        android:layout_toEndOf="@+id/bricht_k_text"        android:layout_toRightOf="@+id/bricht_k_text"        android:text="lx"        android:textColor="#CE000000" />    <ImageView        android:id="@+id/mark_red_to_hot"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_below="@+id/temperature_k_text"        android:layout_alignStart="@+id/too_hot_text_v"        android:layout_alignLeft="@+id/too_hot_text_v"        android:layout_marginStart="11dp"        android:layout_marginLeft="11dp"        android:layout_marginTop="6dp"        android:visibility="invisible"        app:srcCompat="@drawable/mark_red" />    <TextView        android:id="@+id/textView"        android:layout_width="50dp"        android:layout_height="wrap_content"        android:layout_below="@+id/temperature_k_text"        android:layout_marginStart="6dp"        android:layout_marginLeft="6dp"        android:layout_marginTop="6dp"        android:layout_toEndOf="@+id/mark_red_to_hot"        android:layout_toRightOf="@+id/mark_red_to_hot"        android:text="too hot"        android:textColor="#FF0000"        android:textSize="11sp"        tools:visibility="invisible" />    <TextView        android:id="@+id/too_cold_text_v"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_below="@+id/temperature_k_text"        android:layout_alignStart="@+id/textView"        android:layout_alignLeft="@+id/textView"        android:layout_marginStart="0dp"        android:layout_marginLeft="0dp"        android:layout_marginTop="5dp"        android:text="too cold"        android:textColor="#FF0000"        android:textSize="11sp"        tools:visibility="invisible" />    <ImageView        android:id="@+id/mark_yellow"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_below="@+id/co2_k_text"        android:layout_alignStart="@+id/button_hum2"        android:layout_alignLeft="@+id/button_hum2"        android:layout_marginStart="12dp"        android:layout_marginLeft="12dp"        android:layout_marginTop="6dp"        app:srcCompat="@drawable/mark_yellow"        tools:visibility="invisible" />    <TextView        android:id="@+id/bit_dirty"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_below="@+id/co2_k_text"        android:layout_marginStart="4dp"        android:layout_marginLeft="4dp"        android:layout_marginTop="6dp"        android:layout_toEndOf="@+id/mark_yellow"        android:layout_toRightOf="@+id/mark_yellow"        android:text="A bit dirty"        android:textColor="#FFC107"        android:textSize="11sp"        android:visibility="invisible" /></RelativeLayout>